    },
    {
      "Name": "YTDOPY",
      "DisplayFolder": "Growth over full period",
      "MultiLineExpression": [
        "VAR __ValueCurrentPeriod = @@GETMEASURE( YTD )",
        "VAR __ValuePreviousPeriod = @@GETMEASURE( PYC )",
        "VAR __Result =",
